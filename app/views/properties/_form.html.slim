br
.content
  = form_with model: property do |form|
    .form-row
      .form-group.col-md-6
        = form.label :name
        = form.text_field :name, class: "form-control"
      .form-group.col-md-6
        = form.label :presentation
        = form.text_field :presentation, class: "form-control"
    div
      = form.submit class: "btn btn-success"
      = link_to 'Atras', properties_path, class: "btn btn-warning"
